{
  "name": "hazirgiyim-admin-panel",
  "version": "1.0.0",
  "description": "Admin panel for Hazirgiyim Backend",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "clean": "rm -rf dist .medusa/server",
    "build": "npx medusa build",
    "build:backend": "DISABLE_ADMIN=true npx medusa build",
    "build:server": "tsc --build && echo 'Server build completed'",
    "build:prod": "./scripts/build-prod.sh",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "medusa start",
    "dev": "medusa develop",
    "start:prod": "medusa start",
    "start:pm2": "pm2 start ecosystem.config.js --env production",
    "stop:pm2": "pm2 stop ecosystem.config.js",
    "restart:pm2": "pm2 restart ecosystem.config.js",
    "logs:pm2": "pm2 logs medusa-backend",
    "build:admin": "npx medusa admin build",
    "migrate": "medusa migrations run",
    "migrate:revert": "medusa migrations revert",
    "health": "curl -f http://localhost:9000/health || exit 1",
    "railway:deploy": "railway up",
    "railway:logs": "railway logs",
    "railway:run": "railway run",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.9.0",
    "@medusajs/cli": "2.9.0",
    "@medusajs/framework": "2.9.0",
    "@medusajs/medusa": "2.9.0",
    "@medusajs/notification": "^2.9.0",
    "@medusajs/notification-sendgrid": "^2.9.0",
    "@medusajs/payment-stripe": "^2.9.0",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "awilix": "^8.0.1",
    "groq-sdk": "^0.30.0",
    "pg": "^8.13.0",
    "resend": "^6.0.1",
    "sonner": "^2.0.7",
    "stripe": "^18.4.0"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.9.0",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}
